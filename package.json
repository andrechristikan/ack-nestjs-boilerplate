{
  "name": "ack-nestjs-boilerplate",
  "version": "0.2.0",
  "description": "NestJs BoilerPlate With Mongoose",
  "repository": {
    "type": "git",
    "url": "https://github.com/andrechristikan/ack-nestjs-mongoose.git"
  },
  "author": {
    "name": "andrechristikan",
    "email": "ack@baibay.com"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write '{src,test}/**/*.ts'",
    "format:src": "prettier --write src/**/*.ts",
    "format:test": "prettier --write test/**/*.ts",
    "rollback": "nestjs-command remove:user && nestjs-command remove:role && nestjs-command remove:permission",
    "migrate": "nestjs-command insert:permission && nestjs-command insert:role && nestjs-command insert:user",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --ext .ts,.tsx '{src,test}/**/*.ts' --no-error-on-unmatched-pattern",
    "lint:fix": "eslint --ext .ts,.tsx '{src,test}/**/*.ts' --fix --no-error-on-unmatched-pattern",
    "lint:test": "eslint --ext .ts,.tsx test/**/*.ts --no-error-on-unmatched-pattern",
    "lint:src": "eslint --ext .ts,.tsx src/**/*.ts --no-error-on-unmatched-pattern",
    "test": "jest --config test/jest.json --detectOpenHandles --passWithNoTests --forceExit",
    "test:watch": "jest --config test/jest.json --watch",
    "test:cov": "jest --config test/jest.json --coverage",
    "test:debug": "node --config test/jest.json --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.47.0",
    "@faker-js/faker": "^6.0.0-alpha.3",
    "@nestjs/common": "^8.2.5",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.5",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^9.0.2",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.2.5",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "dot-object": "^2.1.4",
    "express-rate-limit": "^6.1.0",
    "helmet": "^5.0.1",
    "moment": "^2.29.1",
    "mongoose": "^6.1.6",
    "morgan": "^1.10.0",
    "nest-winston": "^1.6.2",
    "nestjs-command": "^3.1.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rotating-file-stream": "^3.0.2",
    "rxjs": "^7.5.2",
    "winston": "^3.4.0",
    "winston-daily-rotate-file": "^4.5.5",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.5",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/dot-object": "^2.1.2",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^6.0.0",
    "@types/jest": "^27.4.0",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.8",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "supertest": "^6.2.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  }
}
