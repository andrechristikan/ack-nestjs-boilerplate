{
    "name": "ack-nestjs-boilerplate",
    "version": "8.1.1",
    "description": "Ack NestJs Boilerplate",
    "repository": {
        "type": "git",
        "url": "https://github.com/andrechristikan/ack-nestjs-boilerplate.git"
    },
    "author": {
        "name": "andrechristikan",
        "email": "andrechristikan@gmail.com"
    },
    "private": true,
    "license": "MIT",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "prebuild": "rimraf dist",
        "prepare": "husky",
        "clean": "rimraf dist && rimraf node_modules && pnpm store prune",
        "generate:keys": "npx ts-node scripts/generate-keys.ts",
        "format": "prettier --write '{src,test}/**/*.{ts,json,yml,yaml}' && prettier --write '*.{ts,json,yml,yaml,mjs}'",
        "deadcode": "ts-prune --project tsconfig.json --skip *.json --skip generated/* | grep -v 'used in module'",
        "spell": "cspell lint --config cspell.json '{src,test}/**/*.ts' --color --gitignore --no-must-find-files --no-summary --no-progress || true",
        "typecheck": "tsc --noEmit",
        "typecheck:watch": "tsc --noEmit --watch",
        "build": "nest build application --config nest-cli.json",
        "build:dev": "nest build application --config nest-cli.json --watch",
        "start": "nest start application --config nest-cli.json",
        "start:dev": "nest start application --watch --config nest-cli.json",
        "start:prod": "node dist/main",
        "lint": "eslint",
        "lint:staged": "lint-staged",
        "lint:fix": "eslint --fix",
        "test": "TZ=UTC jest --config test/jest.json --passWithNoTests --detectOpenHandles",
        "package:upgrade": "npx npm-check-updates -u",
        "package:check": "npx npm-check-updates -i",
        "migration": "nest build migration --config nest-cli.json && node dist/migration.js",
        "migration:seed": "nest build migration --config nest-cli.json && node dist/migration.js apiKey --type seed && node dist/migration.js country --type seed && node dist/migration.js featureFlag --type seed && node dist/migration.js role --type seed && node dist/migration.js termPolicy --type seed && node dist/migration.js user --type seed",
        "migration:remove": "nest build migration --config nest-cli.json && node dist/migration.js apiKey --type remove&& node dist/migration.js featureFlag --type remove && node dist/migration.js user --type remove && node dist/migration.js country --type remove && node dist/migration.js role --type remove && node dist/migration.js termPolicy --type remove",
        "db:migrate": "prisma db push",
        "db:generate": "prisma generate",
        "db:studio": "prisma studio",
        "db:format": "prisma format"
    },
    "dependencies": {
        "@2toad/profanity": "^3.2.0",
        "@aws-sdk/client-s3": "^3.952.0",
        "@aws-sdk/client-ses": "^3.952.0",
        "@aws-sdk/s3-request-presigner": "^3.952.0",
        "@casl/ability": "^6.7.3",
        "@faker-js/faker": "^10.1.0",
        "@keyv/redis": "^5.1.5",
        "@nestjs/axios": "^4.0.1",
        "@nestjs/bullmq": "^11.0.4",
        "@nestjs/cache-manager": "^3.0.1",
        "@nestjs/common": "^11.1.9",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11.1.9",
        "@nestjs/jwt": "^11.0.2",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "^11.1.9",
        "@nestjs/swagger": "^11.2.3",
        "@nestjs/terminus": "^11.0.0",
        "@nestjs/throttler": "^6.5.0",
        "@prisma/client": "^6.19.0",
        "@sentry/nestjs": "^10.30.0",
        "@sentry/profiling-node": "^10.30.0",
        "@smithy/types": "^4.10.0",
        "@supercharge/request-ip": "^1.2.0",
        "axios": "^1.13.2",
        "bcryptjs": "^3.0.3",
        "body-parser": "^2.2.1",
        "bson-objectid": "^2.0.4",
        "bullmq": "^5.66.0",
        "bytes": "^3.1.2",
        "cache-manager": "^7.2.7",
        "case": "^1.6.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.3",
        "compression": "^1.8.1",
        "cors": "^2.8.5",
        "crypto-js": "^4.2.0",
        "dotenv": "^17.2.3",
        "google-auth-library": "^10.5.0",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.3",
        "jwks-rsa": "^3.2.0",
        "keyv": "^5.5.5",
        "lodash": "^4.17.21",
        "luxon": "^3.7.2",
        "mime": "^4.1.0",
        "ms": "^2.1.3",
        "nest-commander": "^3.20.1",
        "nestjs-i18n": "^10.6.0",
        "nestjs-pino": "^4.5.0",
        "nestjs-real-ip": "^3.0.1",
        "otplib": "^12.0.1",
        "papaparse": "^5.5.3",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "pino-http": "^11.0.0",
        "pino-pretty": "^13.1.3",
        "reflect-metadata": "^0.2.2",
        "response-time": "^2.3.4",
        "rotating-file-stream": "^3.2.7",
        "rxjs": "^7.8.2",
        "strip-ansi": "^7.1.2",
        "ua-parser-js": "^2.0.7",
        "uuid": "^13.0.0",
        "verify-apple-id-token": "^3.1.2",
        "yargs": "^18.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^20.2.0",
        "@commitlint/config-conventional": "^20.2.0",
        "@eslint/js": "^9.39.2",
        "@golevelup/ts-jest": "^0.7.0",
        "@nestjs/cli": "^11.0.14",
        "@nestjs/schematics": "^11.0.9",
        "@nestjs/testing": "^11.1.9",
        "@swc/cli": "^0.7.9",
        "@swc/core": "^1.15.5",
        "@swc/jest": "^0.2.39",
        "@types/body-parser": "^1.19.6",
        "@types/bytes": "^3.1.5",
        "@types/compression": "^1.8.1",
        "@types/cors": "^2.8.19",
        "@types/crypto-js": "^4.2.2",
        "@types/express": "^5.0.6",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/lodash": "^4.17.21",
        "@types/luxon": "^3.7.1",
        "@types/ms": "^2.1.0",
        "@types/multer": "^2.0.0",
        "@types/node": "^25.0.2",
        "@types/papaparse": "^5.5.2",
        "@types/passport-jwt": "^4.0.1",
        "@types/response-time": "^2.3.9",
        "@types/supertest": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^8.50.0",
        "@typescript-eslint/parser": "^8.50.0",
        "cspell": "^9.4.0",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "husky": "^9.1.7",
        "jest": "^30.2.0",
        "lint-staged": "^16.2.7",
        "prettier": "^3.7.4",
        "prisma": "^6.19.0",
        "rimraf": "^6.1.2",
        "rxjs-marbles": "^7.0.1",
        "stop-only": "^3.4.4",
        "supertest": "^7.1.4",
        "ts-jest": "^29.4.6",
        "ts-node": "^10.9.2",
        "ts-prune": "^0.10.3",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.50.0"
    },
    "lint-staged": {
        "**/*.ts": [
            "eslint",
            "stop-only --file"
        ],
        "*.{json,yaml}": [
            "prettier --write"
        ]
    },
    "packageManager": "pnpm@10.26.0",
    "engines": {
        "npm": "please-use-pnpm",
        "yarn": "please-use-pnpm",
        "node": ">= 24.11.0",
        "pnpm": ">= 10.25.0"
    },
    "resolutions": {
        "form-data": "^4.0.5",
        "glob": "^13.0.0",
        "jws": "^4.0.1",
        "@babel/helpers": "^7.26.10",
        "js-yaml": "^4.1.1",
        "brace-expansion": "^4.0.1",
        "tmp": "^0.2.4",
        "body-parser": "^2.2.1",
        "minimatch": "^10.0.0"
    }
}
