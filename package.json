{
  "name": "ack-nestjs-boilerplate-mongoose",
  "version": "1.3.13",
  "description": "Ack NestJs BoilerPlate Mongoose",
  "repository": {
    "type": "git",
    "url": "https://github.com/andrechristikan/ack-nestjs-boilerplate-mongoose.git"
  },
  "author": {
    "name": "andrechristikan",
    "email": "ack@baibay.id"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write '{src,test,e2e,integration}/**/*.ts'",
    "format:src": "prettier --write src/**/*.ts",
    "format:test": "prettier --write test/**/*.ts",
    "format:e2e": "prettier --write e2e/**/*.ts",
    "format:integration": "prettier --write integration/**/*.ts",
    "rollback": "nestjs-command remove:user && nestjs-command remove:role && nestjs-command remove:permission && nestjs-command remove:authapis && nestjs-command remove:setting",
    "migrate": "nestjs-command insert:setting && nestjs-command insert:authapis && nestjs-command insert:permission && nestjs-command insert:role && nestjs-command insert:user",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --ext .ts,.tsx '{src,test,e2e,integration}/**/*.ts' --no-error-on-unmatched-pattern",
    "lint:fix": "eslint --ext .ts,.tsx '{src,test,e2e,integration}/**/*.ts' --fix --no-error-on-unmatched-pattern",
    "lint:test": "eslint --ext .ts,.tsx test/**/*.ts --no-error-on-unmatched-pattern",
    "lint:src": "eslint --ext .ts,.tsx src/**/*.ts --no-error-on-unmatched-pattern",
    "lint:e2e": "eslint --ext .ts,.tsx e2e/**/*.ts --no-error-on-unmatched-pattern",
    "lint:integration": "eslint --ext .ts,.tsx integration/**/*.ts --no-error-on-unmatched-pattern",
    "test": "yarn test:unit && yarn test:integration && yarn test:e2e",
    "test:unit": "jest --config test/jest.json --detectOpenHandles --passWithNoTests --forceExit",
    "test:e2e": "jest --config e2e/jest.json --detectOpenHandles --passWithNoTests --forceExit",
    "test:integration": "jest --config integration/jest.json --detectOpenHandles --passWithNoTests --forceExit",
    "prepare": "husky install",
    "deadcode": "ts-prune --project tsconfig.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.86.0",
    "@faker-js/faker": "^6.3.1",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.4.4",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.4",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^9.0.3",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/schedule": "^1.1.0",
    "@nestjs/terminus": "^8.0.6",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "exceljs": "^4.3.0",
    "express-rate-limit": "^6.4.0",
    "helmet": "^5.0.2",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "mongoose": "^6.3.2",
    "morgan": "^1.10.0",
    "nest-winston": "^1.6.2",
    "nestjs-command": "^3.1.1",
    "nestjs-i18n": "^9.0.12",
    "passport": "^0.5.2",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rotating-file-stream": "^3.0.3",
    "rxjs": "^7.5.5",
    "ua-parser-js": "^1.0.2",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.4",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/cron": "^1.7.3",
    "@types/exceljs": "^1.3.0",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^6.0.0",
    "@types/jest": "^27.5.0",
    "@types/lodash": "^4.14.182",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.31",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.12",
    "@types/ua-parser-js": "^0.7.36",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.4",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.1",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "ts-prune": "^0.10.3",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  },
  "resolutions": {
    "minimist": "^1.2.6"
  }
}
