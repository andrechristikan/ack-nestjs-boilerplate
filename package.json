{
  "name": "ack-nestjs-boilerplate",
  "version": "1.0.0",
  "description": "NestJs BoilerPlate With Mongoose",
  "repository": "https://github.com/andrechristikan/ack-nestjs-mongoose.git",
  "author": "andrechristikan <andrechristikan@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "migrate:rollback": "nestjs-command remove:user && nestjs-command remove:role && nestjs-command remove:permission",
    "migrate": "nestjs-command create:permission && nestjs-command create:role && nestjs-command create:user",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --ext .ts src",
    "test": "jest --config test/jest.json",
    "test:watch": "jest --config test/jest.json --watch",
    "test:cov": "jest --config test/jest.json --coverage",
    "test:debug": "node --config test/jest.json --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config e2e/jest.json --detectOpenHandles",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.35.0",
    "@nestjs/common": "7.6.11",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "7.6.11",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/microservices": "^7.6.11",
    "@nestjs/mongoose": "7.2.2",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "7.6.11",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "express-rate-limit": "^5.2.3",
    "helmet": "^4.4.1",
    "kafkajs": "^1.15.0",
    "moment": "^2.29.1",
    "mongoose": "5.11.14",
    "morgan": "^1.10.0",
    "nest-winston": "^1.4.0",
    "nestjs-command": "^2.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rotating-file-stream": "^2.1.3",
    "rxjs": "6.6.3",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "7.5.4",
    "@nestjs/schematics": "7.2.7",
    "@nestjs/testing": "^7.6.12",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/express": "4.17.11",
    "@types/express-rate-limit": "^5.1.1",
    "@types/faker": "^5.1.6",
    "@types/jest": "^27.0.2",
    "@types/mongoose": "5.10.3",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.7",
    "@types/node": "14.14.22",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "faker": "^5.4.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-jest": "26.5.0",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.1.3",
    "husky": "^7.0.0"
  }
}
