{
    "name": "ack-nestjs-boilerplate",
    "version": "7.4.3",
    "description": "Ack NestJs Boilerplate",
    "repository": {
        "type": "git",
        "url": "https://github.com/andrechristikan/ack-nestjs-boilerplate.git"
    },
    "author": {
        "name": "andrechristikan",
        "email": "andrechristikan@gmail.com"
    },
    "private": true,
    "license": "MIT",
    "scripts": {
        "prepare": "husky",
        "prebuild": "rimraf dist",
        "clean": "rimraf dist && rimraf node_modules && yarn cache clean",
        "build": "nest build --config nest-cli.prod.json",
        "build:dev": "nest build",
        "typecheck": "tsc --noEmit",
        "typecheck:watch": "tsc --noEmit --watch",
        "format": "prettier --write '{src,test}/**/*.{ts,json,yml,yaml}' && prettier --write '*.{ts,json,yml,yaml,mjs}'",
        "start": "nest start",
        "start:dev": "clear && nest start --watch",
        "start:prod": "node dist/main",
        "lint": "eslint",
        "lint:staged": "lint-staged",
        "lint:fix": "eslint --fix",
        "test": "TZ=UTC jest --config test/jest.json --passWithNoTests --forceExit",
        "deadcode": "ts-prune --project tsconfig.json --unusedInModule --skip *.json",
        "spell": "cspell lint --config cspell.json '{src,test}/**/*.ts' --color --gitignore --no-must-find-files --no-summary --no-progress || true",
        "package:upgrade": "npx npm-check-updates -u",
        "package:check": "npx npm-check-updates -i",
        "generate:keys": "npx ts-node scripts/generate-keys.ts",
        "migrate:fresh": "yarn migrate:remove && yarn migrate:seed",
        "migrate:seed": "nestjs-command seed:country && nestjs-command seed:apikey && nestjs-command seed:role && nestjs-command seed:user && nestjs-command seed:settings",
        "migrate:remove": "nestjs-command remove:user && nestjs-command remove:country && nestjs-command remove:apikey  && nestjs-command remove:role && nestjs-command remove:settings",
        "migrate:template": "nestjs-command migrate:template",
        "rollback:template": "nestjs-command rollback:template",
        "db:generate": "prisma generate",
        "db:studio": "prisma studio",
        "db:format": "prisma format"
    },
    "dependencies": {
        "@aws-sdk/client-pinpoint": "^3.835.0",
        "@aws-sdk/client-s3": "^3.837.0",
        "@aws-sdk/client-ses": "^3.835.0",
        "@aws-sdk/s3-request-presigner": "^3.837.0",
        "@casl/ability": "^6.7.3",
        "@faker-js/faker": "^9.8.0",
        "@keyv/redis": "^4.4.1",
        "@nestjs/axios": "^4.0.0",
        "@nestjs/bullmq": "^11.0.2",
        "@nestjs/cache-manager": "^3.0.1",
        "@nestjs/common": "^11.1.3",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11.1.3",
        "@nestjs/jwt": "^11.0.0",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "^11.1.3",
        "@nestjs/swagger": "^11.2.0",
        "@nestjs/terminus": "^11.0.0",
        "@nestjs/throttler": "^6.4.0",
        "@prisma/client": "^6.14.0",
        "@prisma/extension-read-replicas": "^0.4.1",
        "@sentry/nestjs": "^9.32.0",
        "@sentry/profiling-node": "^9.32.0",
        "axios": "^1.10.0",
        "bad-words": "^4.0.0",
        "bcryptjs": "^3.0.2",
        "bullmq": "^5.56.0",
        "cache-manager": "^7.0.0",
        "case": "^1.6.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.2",
        "compression": "^1.8.0",
        "crypto-js": "^4.2.0",
        "google-auth-library": "^10.1.0",
        "helmet": "^8.1.0",
        "jwks-rsa": "^3.2.0",
        "keyv": "^5.3.4",
        "luxon": "^3.6.1",
        "nestjs-command": "^3.1.5",
        "nestjs-i18n": "^10.5.1",
        "nestjs-pino": "^4.4.0",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "pino-http": "^10.5.0",
        "pino-pretty": "^13.0.0",
        "reflect-metadata": "^0.2.2",
        "response-time": "^2.3.3",
        "rotating-file-stream": "^3.2.6",
        "rxjs": "^7.8.2",
        "uuid": "^11.1.0",
        "verify-apple-id-token": "^3.1.2",
        "xlsx": "^0.18.5",
        "yargs": "^17.7.2",
        "yarn": "^1.22.22"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@eslint/js": "^9.29.0",
        "@golevelup/ts-jest": "^0.7.0",
        "@nestjs/cli": "^11.0.7",
        "@nestjs/schematics": "^11.0.5",
        "@nestjs/testing": "^11.1.3",
        "@swc/cli": "^0.7.7",
        "@swc/core": "^1.12.7",
        "@swc/jest": "^0.2.38",
        "@types/bad-words": "^3.0.3",
        "@types/bytes": "^3.1.5",
        "@types/compression": "^1.8.1",
        "@types/cors": "^2.8.19",
        "@types/crypto-js": "^4.2.2",
        "@types/express": "^5.0.3",
        "@types/jest": "^30.0.0",
        "@types/lodash": "^4.17.19",
        "@types/luxon": "^3.6.2",
        "@types/ms": "^2.1.0",
        "@types/multer": "^1.4.13",
        "@types/node": "^24.0.4",
        "@types/passport-jwt": "^4.0.1",
        "@types/response-time": "^2.3.9",
        "@types/supertest": "^6.0.3",
        "@types/uuid": "^10.0.0",
        "cspell": "^9.1.2",
        "eslint": "^9.29.0",
        "eslint-config-prettier": "^10.1.5",
        "husky": "^9.1.7",
        "jest": "^30.0.3",
        "lint-staged": "^16.1.2",
        "prettier": "^3.6.1",
        "prisma": "^6.14.0",
        "rimraf": "^6.0.1",
        "rxjs-marbles": "^7.0.1",
        "stop-only": "^3.4.3",
        "supertest": "^7.1.1",
        "ts-jest": "^29.4.0",
        "ts-node": "^10.9.2",
        "ts-prune": "^0.10.3",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.35.0"
    },
    "lint-staged": {
        "**/*.ts": [
            "eslint",
            "stop-only --file"
        ],
        "*.{json,yaml}": [
            "prettier --write"
        ]
    },
    "packageManager": "yarn@1.22.22",
    "engines": {
        "npm": "please-use-yarn",
        "yarn": ">= 1.22.22",
        "node": ">= 20.11.0"
    }
}
