{
  "categories": [
    {
      "title": "## ğŸ¥¶ Breaking Change",
      "labels": ["breaking-change"]
    },
    {
      "title": "## ğŸš€ Features",
      "labels": ["feature"]
    },
    {
      "title": "## ğŸ› Fixes",
      "labels": [
        "fix"
      ]
    },
    {
      "title": "## ğŸ§ª Tests",
      "labels": [
        "test"
      ]
    },
    {
      "title": "## ğŸ“¦ Dependencies",
      "labels": [
        "javascript" ,
        "github_actions",
        "dependencies"
      ]
    },
    {
      "title": "## ğŸ“ Other",
      "labels": ["*"],
      "exclude_labels": [ 
        "test" ,
        "fix", 
        "feature", 
        "breaking-change",
        "javascript" ,
        "github_actions"
      ],
      "exhaustive": true
    }
  ],
  "ignore_labels": [
    "ignore", 
    "stale", 
    "invalid", 
    "duplicate"
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n${{UNCATEGORIZED}}\n</details>",
  "pr_template": "- ${{TITLE}}\n   - PR: #${{NUMBER}}",
  "empty_template": "- no changes",
  "label_extractor": [
    {
      "pattern": ".*(\\[Feature\\]).*",
      "on_property": "title",
      "method": "match"
    },
    {
      "pattern": "\\[Issue\\]",
      "on_property": "title",
      "method": "match"
    }
  ],
  "max_tags_to_fetch": 100,
  "max_pull_requests": 100,
  "max_back_track_time_days": 365,
  "base_branches": [
    "main"
  ]
}
